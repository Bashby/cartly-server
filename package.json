{
  "name": "bodegoserver",
  "version": "0.1.0",
  "description": "Server-side/backend logic for BodegoIO.",
  "main": "server.js",
  "scripts": {
    "test": "mocha --require babel-register --exit",
    "start": "nodemon app/app.js --exec babel-node",
    "start:docker": "nodemon app/app.js --exec babel-node --legacy-watch",
    "build": "babel app -d build",
    "serve": "node build/app.js",
    "lint": "eslint --ignore-path .gitignore .",
    "pretest": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/bodegoio/bodegoserver.git"
  },
  "keywords": [
    "nodejs",
    "express",
    "postgres"
  ],
  "author": "Brendan Evans Ashby",
  "license": "UNLICENSED",
  "homepage": "https://bitbucket.org/bodegoio/bodegoserver#readme",
  "dependencies": {
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "express": "^4.16.2",
    "morgan": "^1.9.0",
    "pg-promise": "^7.5.4",
    "serve-static": "^1.13.2",
    "validator": "^9.4.1",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "eslint": "^4.18.1",
    "mocha": "^5.0.1",
    "nodemon": "^1.15.1"
  }
}
