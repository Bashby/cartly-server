{
  "name": "cartlyserver",
  "version": "0.1.0",
  "description": "Server-side/backend logic for CartlyIO.",
  "main": "server.js",
  "scripts": {
    "test": "mocha --require babel-register --exit",
    "start": "nodemon app/app.js --exec babel-node",
    "start:docker": "nodemon app/app.js --exec babel-node --legacy-watch",
    "build": "babel app -d build",
    "serve": "node build/app.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/cartlyio/cartlyserver.git"
  },
  "keywords": [
    "nodejs",
    "express",
    "postgres"
  ],
  "author": "Brendan Evans Ashby",
  "license": "UNLICENSED",
  "homepage": "https://bitbucket.org/cartlyio/cartlyserver#readme",
  "dependencies": {
    "bluebird": "^3.5.1",
    "express": "^4.16.2",
    "pg-promise": "^7.4.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "mocha": "^5.0.0",
    "nodemon": "^1.14.12"
  }
}
